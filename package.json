    "typeorm:cli": "env-cmd ts-node --transpile-only ./node_modules/typeorm/cli.js -d ./ormconfig.ts",
    "db:drop": "npm run typeorm:cli schema:drop",
    "migration:create": "typeorm  migration:create libs/migrations/src/<migration_file_name>",
    "migration:run": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert",
    "migration:show": "npm run typeorm:cli -- migration:show",
    "start:api:production": "npm run migration:run && node dist/apps/api/main.js",
    "codegen-admin": "env-cmd graphql-codegen -c ./apps/admin/src/codegen.ts --watch"
